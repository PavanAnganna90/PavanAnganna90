    /**
   * Get severity configuration
   */ const getSeverityConfig = (severity)=>{
        switch(severity){
            case 'CRITICAL':
                return {
                    status: 'error',
                    color: 'text-red-600 dark:text-red-400',
                    bgColor: 'bg-red-50 dark:bg-red-900/20',
