                                        children: [
                                            /*#__PURE__*/ (0, _jsxruntime.jsxs)("span", {
                                                className: "text-sm text-gray-600 dark:text-gray-400",
                                                children: [
                                                    selectedAlerts.size,
                                                    " selected"
                                                ]
                                            }),
                                            /*#__PURE__*/ (0, _jsxruntime.jsx)(_ui.Button, {
                                                size: "sm",
                                                variant: "outline",
                                                onClick: ()=>handleBulkAction('acknowledge'),
                                                children: "Acknowledge"
                                            }),
                                            /*#__PURE__*/ (0, _jsxruntime.jsx)(_ui.Button, {
                                                size: "sm",
                                                variant: "outline",
                                                onClick: ()=>handleBulkAction('resolve'),
                                                children: "Resolve"
                                            })
                                        ]
                                    }),
                                    /*#__PURE__*/ (0, _jsxruntime.jsx)(_ui.Button, {
                                        size: "sm",
                                        onClick: ()=>loadAlerts(true),
                                        disabled: loading,
                                        children: "Refresh"
                                    })
                                ]
                            })
                        ]
                    }),
                    /*#__PURE__*/ (0, _jsxruntime.jsx)(_AlertFilters.default, {
                        filter: filter,
                        onFilterChange: handleFilterChange,
                        sort: sort,
                        onSortChange: handleSortChange,
                        compact: compact
                    }),
                    alerts.length === 0 ? /*#__PURE__*/ (0, _jsxruntime.jsxs)("div", {
                        className: "text-center py-12",
                        children: [
                            /*#__PURE__*/ (0, _jsxruntime.jsx)("svg", {
                                className: "mx-auto h-12 w-12 text-gray-400",
                                fill: "none",
                                stroke: "currentColor",
                                viewBox: "0 0 24 24",
                                children: /*#__PURE__*/ (0, _jsxruntime.jsx)("path", {
                                    strokeLinecap: "round",
                                    strokeLinejoin: "round",
                                    strokeWidth: 2,
                                    d: "M9 12l2 2 4-4m6 2a9 9 0 11-18 0 9 9 0 0118 0z"
                                })
                            }),
                            /*#__PURE__*/ (0, _jsxruntime.jsx)("h3", {
                                className: "mt-2 text-sm font-medium text-gray-900 dark:text-white",
                                children: "No alerts found"
                            }),
                            /*#__PURE__*/ (0, _jsxruntime.jsx)("p", {
                                className: "mt-1 text-sm text-gray-500 dark:text-gray-400",
                                children: Object.keys(filter).length > 0 ? 'Try adjusting your filters.' : 'All clear! No alerts to display.'
                            })
                        ]
                    }) : /*#__PURE__*/ (0, _jsxruntime.jsx)("div", {
                        className: "space-y-3",
                        children: alerts.map((alert)=>/*#__PURE__*/ (0, _jsxruntime.jsx)(_AlertCard.default, {
                                alert: alert,
                                compact: compact,
                                selected: selectedAlerts.has(alert.id),
                                onSelect: ()=>toggleAlertSelection(alert.id),
                                onAction: handleAlertAction,
                                onViewDetails: ()=>setSelectedAlert(alert.id),
                                showActions: true
                            }, alert.id))
                    }),
                    hasCIAlerts() && !compact && /*#__PURE__*/ (0, _jsxruntime.jsxs)("div", {
                        className: "mt-6",
                        children: [
                            /*#__PURE__*/ (0, _jsxruntime.jsx)("h3", {
                                className: "text-lg font-semibold text-gray-900 dark:text-white mb-4",
                                children: "CI/CD Failure Analysis"
                            }),
                            /*#__PURE__*/ (0, _jsxruntime.jsx)(_CIFailureAnalyzer.default, {
                                compact: true
                            })
                        ]
                    }),
                    alertData && alertData.total > maxAlerts && /*#__PURE__*/ (0, _jsxruntime.jsxs)("div", {
                        className: "flex items-center justify-between",
                        children: [
                            /*#__PURE__*/ (0, _jsxruntime.jsxs)("p", {
                                className: "text-sm text-gray-700 dark:text-gray-300",
                                children: [
                                    "Showing ",
                                    alerts.length,
                                    " of ",
                                    alertData.total,
                                    " alerts"
                                ]
                            }),
                            /*#__PURE__*/ (0, _jsxruntime.jsxs)("div", {
                                className: "flex items-center gap-2",
                                children: [
                                    /*#__PURE__*/ (0, _jsxruntime.jsx)(_ui.Button, {
                                        size: "sm",
                                        variant: "outline",
                                        onClick: ()=>setPage((p)=>Math.max(1, p - 1)),
                                        disabled: page === 1,
                                        children: "Previous"
                                    }),
                                    /*#__PURE__*/ (0, _jsxruntime.jsxs)("span", {
                                        className: "text-sm text-gray-600 dark:text-gray-400",
                                        children: [
                                            "Page ",
                                            page
                                        ]
                                    }),
                                    /*#__PURE__*/ (0, _jsxruntime.jsx)(_ui.Button, {
                                        size: "sm",
                                        variant: "outline",
                                        onClick: ()=>setPage((p)=>p + 1),
                                        disabled: !alertData.has_next,
                                        children: "Next"
                                    })
                                ]
                            })
                        ]
                    })
                ]
            }),
            selectedAlert && /*#__PURE__*/ (0, _jsxruntime.jsx)(_AlertDetailModal.default, {
                alertId: selectedAlert,
                onClose: ()=>setSelectedAlert(null),
                onAction: handleAlertAction
            })
        ]
    });
};
const _default = AlertSummary;

