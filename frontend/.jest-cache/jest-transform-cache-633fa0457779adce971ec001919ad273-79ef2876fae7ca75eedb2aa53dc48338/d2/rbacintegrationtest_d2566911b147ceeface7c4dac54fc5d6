                    'view_users',
                    'manage_users'
                ],
                'roles': [
                    'view_roles',
                    'manage_roles'
                ]
            };
            const requiredPermissions = resourcePermissions[resource] || [];
            return requiredPermissions.some((perm)=>user.permissions.includes(perm));
        }),
