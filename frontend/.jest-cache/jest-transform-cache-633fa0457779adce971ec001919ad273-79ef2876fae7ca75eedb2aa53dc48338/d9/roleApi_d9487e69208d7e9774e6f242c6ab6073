class RoleApiService {
    constructor(){
        this.baseUrl = process.env.NODE_ENV === 'production' ? '/api/v1/roles' : 'http://localhost:8000/api/v1/roles';
    }
    getAuthToken() {
        return localStorage.getItem('authToken');
    }
    async request(endpoint, options = {}) {
        const token = this.getAuthToken();
        const url = `${this.baseUrl}${endpoint}`;
        const defaultHeaders = {
            'Content-Type': 'application/json'
        };
        if (token) {
            defaultHeaders['Authorization'] = `Bearer ${token}`;
        }
        const config = {
            ...options,
            headers: {
                ...defaultHeaders,
                ...options.headers
            }
        };
        try {
            const response = await fetch(url, config);
            if (response.status === 401) {
                throw new Error('Authentication required. Please log in again.');
            }
            if (response.status === 403) {
                throw new Error('Access denied. You do not have permission to perform this action.');
            }
            if (!response.ok) {
                const errorText = await response.text();
                throw new Error(`HTTP ${response.status}: ${errorText || response.statusText}`);
            }
            return await response.json();
        } catch (error) {
            console.error(`Role API request failed: ${url}`, error);
            throw error;
        }
    }
    /**
   * Get all roles
   */ async getRoles(includePermissions = false) {
        const queryParams = includePermissions ? '?include_permissions=true' : '';
        return this.request(`${queryParams}`);
    }
    /**
   * Get a specific role by ID
   */ async getRole(roleId) {
        return this.request(`/${roleId}`);
    }
    /**
   * Create a new role
   */ async createRole(roleData) {
        return this.request('', {
            method: 'POST',
            body: JSON.stringify(roleData)
        });
    }
    /**
   * Update an existing role
   */ async updateRole(roleId, roleData) {
        return this.request(`/${roleId}`, {
            method: 'PUT',
            body: JSON.stringify(roleData)
        });
    }
    /**
   * Delete a role
   */ async deleteRole(roleId) {
        return this.request(`/${roleId}`, {
            method: 'DELETE'
        });
    }
    /**
   * Get all permissions
   */ async getPermissions() {
        return this.request('/permissions');
    }
    /**
   * Get permissions organized by category
   */ async getPermissionCategories() {
        return this.request('/permissions/categories');
    }
    /**
   * Get role permissions
   */ async getRolePermissions(roleId) {
        return this.request(`/${roleId}/permissions`);
    }
    /**
   * Update role permissions
   */ async updateRolePermissions(roleId, permissionIds) {
        return this.request(`/${roleId}/permissions`, {
            method: 'PUT',
            body: JSON.stringify({
                permission_ids: permissionIds
            })
        });
    }
    /**
   * Assign role to user
   */ async assignRoleToUser(assignmentData) {
        return this.request('/assign', {
            method: 'POST',
            body: JSON.stringify(assignmentData)
        });
    }
    /**
   * Remove role from user
   */ async removeRoleFromUser(userId, roleId) {
        return this.request(`/unassign`, {
            method: 'POST',
            body: JSON.stringify({
                user_id: userId,
                role_id: roleId
            })
        });
    }
    /**
   * Get user's effective permissions
   */ async getUserPermissions(userId) {
        return this.request(`/users/${userId}/permissions`);
    }
    /**
   * Get current user's permissions
   */ async getCurrentUserPermissions() {
        return this.request('/users/me/permissions');
    }
    /**
   * Check if user has specific permission
   */ async checkUserPermission(userId, permission, teamId) {
        const queryParams = new URLSearchParams({
            permission,
            ...teamId && {
                team_id: teamId.toString()
            }
        });
        return this.request(`/users/${userId}/check-permission?${queryParams}`);
    }
    /**
   * Get role analytics
   */ async getRoleAnalytics() {
        return this.request('/analytics');
    }
    /**
   * Get permission matrix for all roles
   */ async getPermissionMatrix() {
        return this.request('/matrix');
    }
    /**
   * Compare two roles
   */ async compareRoles(role1Id, role2Id) {
        return this.request(`/compare/${role1Id}/${role2Id}`);
    }
    /**
   * Bulk role operations
   */ async bulkRoleOperation(operation) {
        return this.request('/bulk', {
            method: 'POST',
            body: JSON.stringify(operation)
        });
    }
    /**
   * Get role suggestions for users
   */ async getRoleSuggestions() {
        return this.request('/suggestions');
    }
    /**
   * Get RBAC health check
   */ async getRBACHealth() {
        return this.request('/health');
    }
    /**
   * Get users with specific role
   */ async getUsersWithRole(roleId) {
        return this.request(`/${roleId}/users`);
    }
    /**
   * Clone a role
   */ async cloneRole(roleId, newName) {
        return this.request(`/${roleId}/clone`, {
            method: 'POST',
            body: JSON.stringify({
                name: newName
            })
        });
    }
    /**
   * Get role hierarchy
   */ async getRoleHierarchy() {
        return this.request('/hierarchy');
    }
    /**
   * Export roles configuration
   */ async exportRoles() {
        return this.request('/export');
    }
    /**
   * Import roles configuration
   */ async importRoles(configData) {
        return this.request('/import', {
            method: 'POST',
            body: JSON.stringify(configData)
        });
    }
    /**
   * Validate role configuration
   */ async validateRoleConfig() {
        return this.request('/validate');
    }
}
const roleApi = new RoleApiService();
const _default = roleApi;

