        it('updates cost range filters correctly', async ()=>{
            const mockOnFiltersChange = jest.fn();
            (0, _react1.render)(/*#__PURE__*/ (0, _jsxruntime.jsx)(_AwsCostMetricsPanel.AwsCostMetricsPanel, {
                ...defaultProps,
                onFiltersChange: mockOnFiltersChange
            }));
            const filterButton = _react1.screen.getByRole('button', {
                name: /filters/i
            });
            _react1.fireEvent.click(filterButton);
            // Set min cost
            const minInput = _react1.screen.getByPlaceholderText('Min');
            _react1.fireEvent.change(minInput, {
                target: {
                    value: '100'
                }
            });
            await (0, _react1.waitFor)(()=>{
                expect(mockOnFiltersChange).toHaveBeenCalledWith(expect.objectContaining({
                    costThreshold: expect.objectContaining({
                        min: 100
                    })
                }));
            });
            // Set max cost
            const maxInput = _react1.screen.getByPlaceholderText('Max');
            _react1.fireEvent.change(maxInput, {
                target: {
                    value: '2000'
                }
            });
            await (0, _react1.waitFor)(()=>{
                expect(mockOnFiltersChange).toHaveBeenCalledWith(expect.objectContaining({
                    costThreshold: expect.objectContaining({
                        max: 2000
                    })
                }));
            });
        });
