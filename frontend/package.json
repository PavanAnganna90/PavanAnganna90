{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,css,md}\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --coverage --watchAll=false --passWithNoTests --maxWorkers=2 --verbose",
    "test:coverage": "jest --coverage",
    "coverage": "jest --coverage --watchAll=false",
    "coverage:analyze": "node scripts/coverage-analysis.js run",
    "coverage:report": "node scripts/coverage-analysis.js analyze",
    "coverage:ci": "jest --coverage --watchAll=false --maxWorkers=2 --coverageReporters=lcov --coverageReporters=json --coverageReporters=text-summary",
    "type-check": "tsc --noEmit",
    "validate": "npm run lint && npm run format:check && npm run type-check && npm run test",
    "analyze": "ANALYZE=true npm run build",
    "analyze:server": "npm run build && npx next build --analyze",
    "lighthouse:local": "lhci autorun",
    "lighthouse:desktop": "lhci autorun --config=lighthouserc-desktop.js",
    "lighthouse:audit": "npm run build && npm run lighthouse:local",
    "performance:test": "npm run lighthouse:audit && npm run lighthouse:desktop",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:chromium": "playwright test --project=chromium",
    "test:e2e:firefox": "playwright test --project=firefox",
    "test:e2e:safari": "playwright test --project=webkit",
    "test:e2e:mobile": "playwright test --project='Mobile*'",
    "test:e2e:accessibility": "playwright test --project='accessibility*'",
    "test:e2e:performance": "playwright test --project='performance*'",
    "test:e2e:install": "playwright install",
    "test:cross-browser": "playwright test --project=chromium --project=firefox --project=webkit --project='Microsoft Edge'",
    "test:security": "playwright test tests/security/ --reporter=html",
    "test:security-headers": "playwright test tests/security/security-audit.spec.ts --grep=\"Security Headers\"",
    "test:security-xss": "playwright test tests/security/security-audit.spec.ts --grep=\"XSS\"",
    "test:security-comprehensive": "playwright test tests/security/security-audit.spec.ts --grep=\"Comprehensive Security Report\"",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test:unit": "jest --testPathPattern=\"src/.*\\.test\\.(ts|tsx)$\" --coverage",
    "test:integration": "jest --testPathPattern=\"tests/integration/.*\\.test\\.(ts|tsx)$\" --coverage",
    "docker:build": "docker build -t opssight:latest .",
    "docker:build:dev": "docker build --target development -t opssight:dev .",
    "docker:build:test": "docker build --target testing -t opssight:test .",
    "docker:run": "docker run -p 3000:3000 opssight:latest",
    "docker:run:dev": "docker run -p 3000:3000 -v $(pwd):/app opssight:dev",
    "docker:up": "docker-compose up -d",
    "docker:up:dev": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml up -d",
    "docker:up:prod": "docker-compose -f docker-compose.yml -f docker-compose.prod.yml up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:clean": "docker system prune -a -f",
    "ci:validate": "npm run lint && npm run type-check && npm run format:check",
    "ci:test": "npm run test:unit && npm run test:integration && npm run test:security",
    "ci:build": "npm run build",
    "ci:e2e": "npm run test:e2e",
    "ci:lighthouse": "npm run lighthouse:audit",
    "deploy": "./.github/scripts/deploy.sh",
    "deploy:staging": "ENVIRONMENT=staging ./.github/scripts/deploy.sh",
    "deploy:production": "ENVIRONMENT=production ./.github/scripts/deploy.sh",
    "health-check": "curl -f http://localhost:3000/api/health || exit 1",
    "smoke-test": "npm run health-check && curl -f http://localhost:3000/dashboard",
    "precommit": "npm run lint:fix && npm run format && npm run type-check",
    "prepush": "npm run validate",
    "release:patch": "npm version patch && git push && git push --tags",
    "release:minor": "npm version minor && git push && git push --tags",
    "release:major": "npm version major && git push && git push --tags"
  },
  "dependencies": {
    "@emotion/babel-plugin": "^11.13.5",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@headlessui/react": "^2.2.4",
    "@heroicons/react": "^2.2.0",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-toast": "^1.2.14",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@sentry/nextjs": "^8.45.0",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/postcss": "^4.1.8",
    "@tailwindcss/typography": "^0.5.16",
    "@tanstack/react-query": "^5.80.6",
    "@tanstack/react-query-devtools": "^5.80.6",
    "@tremor/react": "^3.18.7",
    "@types/pg": "^8.15.4",
    "@types/react-router-dom": "^5.3.3",
    "axios": "^1.9.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "critters": "^0.0.25",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.16.0",
    "ioredis": "^5.6.1",
    "jose": "^6.0.11",
    "lucide-react": "^0.513.0",
    "next": "15.3.3",
    "next-auth": "^4.24.11",
    "next-themes": "^0.4.6",
    "pg": "^8.16.3",
    "prom-client": "^15.1.3",
    "prometheus-client": "^0.1.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-error-boundary": "^6.0.0",
    "react-helmet-async": "^2.0.5",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.6.2",
    "recharts": "^2.15.3",
    "redis": "^5.6.0",
    "socket.io-client": "^4.8.1",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.2",
    "@axe-core/react": "^4.10.2",
    "@chromatic-com/storybook": "^4.0.0",
    "@eslint/eslintrc": "^3",
    "@lhci/cli": "^0.14.0",
    "@next/bundle-analyzer": "^15.3.3",
    "@playwright/test": "^1.52.0",
    "@storybook/addon-a11y": "^9.0.5",
    "@storybook/addon-docs": "^9.0.5",
    "@storybook/addon-onboarding": "^9.0.5",
    "@storybook/addon-vitest": "^9.0.5",
    "@storybook/nextjs-vite": "^9.0.5",
    "@testing-library/jest-dom": "^6.1.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.0.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^22.15.30",
    "@types/react": "^19.1.7",
    "@types/react-dom": "^19.1.6",
    "@vitest/browser": "^3.2.2",
    "@vitest/coverage-v8": "^3.2.2",
    "autoprefixer": "^10.4.20",
    "axe-core": "^4.10.3",
    "eslint": "^9",
    "eslint-config-next": "15.3.3",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^9.0.5",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lighthouse": "^12.6.1",
    "playwright": "^1.52.0",
    "postcss": "^8.5.0",
    "prettier": "^3.1.0",
    "storybook": "^9.0.5",
    "tailwindcss": "^4.1.8",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.33.1",
    "vitest": "^3.2.2"
  }
}
